{
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "Freeweb",
    "name": "FreeWeb",
    "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
    "browser_action": {
          "default_icon": {
            "16": "icon16.png",
            "24": "icon24.png",
            "32": "icon32.png"
          },
          "default_title": "FreeWeb"
        },
    "permissions": [
        "proxy",
        "tabs",
        "unlimitedStorage",
        "storage",
        "<all_urls>",
        "webRequest",
        "webRequestBlocking"
    ],
    "background": {
        "scripts": ["proxy.js"]
    },
    "minimum_chrome_version":"22.0.0"
}
